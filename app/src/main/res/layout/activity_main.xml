<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#194F82"
    android:fitsSystemWindows="true"
    >
    <RelativeLayout
        android:id="@+id/btn6"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="#194F82"
        android:layout_marginLeft="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        app:layout_constraintLeft_toRightOf="@id/btn5">

        <ImageView
            android:id="@+id/buttonimage6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="-16dp"
            tools:layout_editor_absoluteY="-92dp" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/btn5"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="#194F82"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_goneMarginRight="20dp"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/buttonimage5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="-16dp"
            tools:layout_editor_absoluteY="-92dp" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/btn4"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="#194F82"
        android:layout_marginRight="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        app:layout_constraintRight_toLeftOf="@id/btn5">

        <ImageView
            android:id="@+id/buttonimage4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="-16dp"
            tools:layout_editor_absoluteY="-92dp" />
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/btn3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="25dp"
        android:layout_marginBottom="25dp"
        android:background="#194F82"
        app:layout_constraintLeft_toRightOf="@id/btn2"
        app:layout_constraintVertical_bias="0.321"
        app:layout_constraintBottom_toTopOf="@id/btn6"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/buttonimage3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="-16dp"
            tools:layout_editor_absoluteY="-92dp" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/btn2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginBottom="25dp"
        android:background="#194F82"
        app:layout_constraintBottom_toTopOf="@id/btn5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/buttonimage2"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/btn1"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="#194F82"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.14"
        app:layout_constraintRight_toLeftOf="@id/btn2"
        app:layout_constraintBottom_toTopOf="@id/btn4"
        app:layout_constraintVertical_bias="0.321"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/buttonimage1"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/btn7"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginRight="25dp"
        android:layout_marginTop="25dp"
        android:background="#194F82"
        app:layout_constraintRight_toLeftOf="@id/btn8"
        app:layout_constraintTop_toBottomOf="@id/btn4"
        app:layout_constraintVertical_bias="0.68"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/buttonimage7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="-16dp"
            tools:layout_editor_absoluteY="-92dp" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/btn9"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="#194F82"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="25dp"
        app:layout_constraintLeft_toRightOf="@id/btn8"
        app:layout_constraintVertical_bias="0.68"
        app:layout_constraintTop_toBottomOf="@id/btn6"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/buttonimage9"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="-16dp"
            tools:layout_editor_absoluteY="-92dp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="330dp"
        android:layout_height="8dp"
        android:background="#FFC10D"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        app:layout_constraintBottom_toTopOf="@id/btn5"
        android:layout_marginBottom="10dp"/>

    <RelativeLayout
        android:layout_width="330dp"
        android:layout_height="8dp"
        android:background="#FFC10D"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toBottomOf="@id/btn5"
        android:layout_marginTop="10dp"/>

    <RelativeLayout
        android:layout_width="8dp"
        android:layout_height="350dp"
        android:layout_marginLeft="8dp"
        android:background="#FFC10D"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/btn5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.501"
        tools:ignore="MissingConstraints" />

    <RelativeLayout
        android:layout_width="8dp"
        android:layout_height="350dp"
        android:layout_marginRight="10dp"
        android:background="#FFC10D"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircleRadius="20dp"
        app:layout_constraintRight_toLeftOf="@id/btn5"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/restart"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#56ACE0"
        android:text="restart"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintRight_toLeftOf="@id/btn8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn8"
        app:layout_constraintVertical_bias="0.557"
        tools:ignore="MissingConstraints" />

    <RelativeLayout
        android:id="@+id/btn8"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="25dp"
        android:background="#194F82"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn5"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/buttonimage8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="-16dp"
            tools:layout_editor_absoluteY="-92dp" />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="274dp"
        android:layout_height="72dp"
        android:orientation="vertical"
        android:paddingLeft="40dp"
        android:paddingRight="40dp"
        app:layout_constraintBottom_toTopOf="@+id/btn2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.701"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/scorex"
            android:layout_width="206dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/caramelsweets"
            android:gravity="center_horizontal"
            android:text="Score X : 0"
            android:textAlignment="center"
            android:textColor="#FFC10D"
            android:textSize="30dp" />

        <TextView
            android:id="@+id/scorey"
            android:layout_width="207dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/caramelsweets"
            android:gravity="center_horizontal"
            android:text="Score O : 0"
            android:textAlignment="center"
            android:textColor="#FFC10D"
            android:textSize="30dp" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>